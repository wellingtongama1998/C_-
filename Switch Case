main(){
	
	cout << "Escolha as notas: \n";
	cout << "1=R$100 , 2=R$50, 3=R$20, 4=R$10" << endl;
	cout << "5=R$5 , 6=2, 7=1" << endl;
	cout << "Digite um valor";
	cin >> valor;
	
	do{ 
	switch (valor){
		case 1:
			cout << "R$100";
			break;
		case 2:
			cout << "R$50";
			break;
		case 3:
			cout << "R$20";
			break;
		case 4:
			cout << "R$10";
			break;
		case 5:
			cout << "R$5";
			break;
		case 6:
			cout << "R$2";
			break;
		case 7:
			cout << "R$1";
			break;
		
	}}while(invalido){
		
	}

	
	return 0;
}
